{
  "manifest_version": 3,
  "name": "TIME2DO",
  "version": "1.0.1",
  "description": "타이머와 할일 목록으로 내 작업시간을 체크할 수 있어요.",
  "icons": {
    "16": "icons/Time2DoLogo16.png",
    "48": "icons/Time2DoLogo48.png",
    "128": "icons/Time2DoLogo128.png"
  },
  "sidebar_action": {
    "default_title": "TIME2DO",
    "default_icon": {
